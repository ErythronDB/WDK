#!/usr/bin/perl

use lib "$ENV{GUS_HOME}/lib/perl";
use WDK::Model::CommandHelper;

my $GUS_HOME = $ENV{GUS_HOME};
my $CLASSPATH = &WDK::Model::CommandHelper::getJavaClasspath($GUS_HOME);
my $args = &WDK::Model::CommandHelper::getJavaArgs(@ARGV);

my $cmd = "java -DcmdName=wdkBooleanQuestion -DconfigDir=$GUS_HOME/config -DschemaFile=$GUS_HOME/lib/rng/wdkModel.rng -classpath $CLASSPATH org.gusdb.wdk.model.test.BooleanQuestionTester $args";

system($cmd);
