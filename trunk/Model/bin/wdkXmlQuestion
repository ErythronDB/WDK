#!/usr/bin/perl

use lib "$ENV{GUS_HOME}/lib/perl";
use WDK::Model::CommandHelper;

my $GUS_HOME = $ENV{GUS_HOME};
my $CLASSPATH = &WDK::Model::CommandHelper::getJavaClasspath($GUS_HOME);
my $args = &WDK::Model::CommandHelper::getJavaArgs(@ARGV);

my $cmd = "java  -DconfigDir=$GUS_HOME/config -DcmdName=wdkSummary -DschemaFile=$GUS_HOME/lib/rng/wdkModel.rng -DxmlSchemaFile=$GUS_HOME/lib/rng/xmlAnswer.rng -DxmlDataDir=$GUS_HOME/data/ -classpath $CLASSPATH org.gusdb.wdk.model.test.XmlQuestionTester $args";

system($cmd);
